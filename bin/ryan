#!/usr/bin/env reia

# load([code::lib_dir().to_string(), 'ryan', 'retem.re'].join('/'))

puts('Starting up Ryan...')
ryan_dir = [code::lib_dir().to_string(), 'ryan'].join('/')

required = ['retem.re', 'ryan.re', 'session.re']
required.each { |lib| puts(['Loading', lib].join(' ')); load([ryan_dir, lib].join('/')) }

# Initializing application
load('startup.re')
Startup.initialize()

yaws_shim::init_yaws()
